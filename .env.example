# Core model auth
ANTHROPIC_API_KEY=your_anthropic_api_key
# Anthropic-compatible MiniMax endpoint
ANTHROPIC_API_HOST=https://api.minimax.io/anthropic
# Keep this in sync for SDKs that read base URL instead of API_HOST.
ANTHROPIC_BASE_URL=https://api.minimax.io/anthropic

# Discord bot auth
DISCORD_TOKEN=your_discord_bot_token

# Optional Discord app creds (if you use them elsewhere)
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret

# Optional live-test channel for sending messages in tests
DISCORD_TEST_CHANNEL_ID=123456789012345678

# Optional model override for live agent-loop test
# Example: anthropic:claude-sonnet-4-5-20250929
OPEN_STRIX_TEST_MODEL=
